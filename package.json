{
  "name": "micro-es7-shim",
  "version": "0.1.0",
  "description": "No-bullshit super-simple ESnext shim for Node.js LTS",
  "keywords": [
    "esnext",
    "shim",
    "polyfill",
    "object",
    "entries",
    "values",
    "promise",
    "try",
    "finally"
  ],
  "bugs": "https://github.com/paulmillr/micro-es7-shim/issues",
  "license": "MIT",
  "author": "Aleksey Shvayka, Paul Miller (http://paulmillr.com)",
  "repository": "paulmillr/micro-es7-shim",
  "scripts": {
    "test:object": "test262-harness test262/test/**/*.js --prelude index.js",
    "test:promise": "mocha --require index.js",
    "test": "npm run test:object && npm run test:promise"
  },
  "devDependencies": {
    "test262-harness": "hax/test262-harness#patch-1",
    "mocha": "^3"
  },
  "engines": {
    "node": ">= 6.10"
  }
}
